{
  "compilerOptions": {
    "types": ["node", "cesium", "vite/client"],
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "./src",
    "checkJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
